 return () => stopSync();\r\n  }, []);\r\n\r\n  return (\r\n    <div className={styles.app}>\r\n      <BrowserRouter>\r\n        <Header logout={logout} userData={userData} />\r\n        <Switch>\r\n          <Route exact path=\"/\">\r\n            {items ? <MainPage items={items} /> : <Loading />}\r\n            <Footer />\r\n          </Route>\r\n          <Route exact path=\"/intro\">\r\n            <Intro />\r\n          </Route>\r\n          <Route exact path=\"/notice\">\r\n            {notices ? (\r\n              <Notice\r\n                noticeRepository={noticeRepository}\r\n                notices={notices}\r\n                authService={authService}\r\n                adminId={adminId}\r\n              />\r\n            ) : (\r\n              <Loading />\r\n            )}\r\n          </Route>\r\n          <Route exact path=\"/itemlist\">\r\n            {items ? (\r\n              <ItemListPage items={items} userData={userData} />\r\n            ) : (\r\n              <Loading />\r\n            )}\r\n          </Route>\r\n          <Route exact path=\"/event\">\r\n            <EventPage />\r\n          </Route>\r\n          <Route exact path=\"/bbs\">\r\n            {articles ? (\r\n              <Bbs articles={articles} authService={authService} />\r\n            ) : (\r\n              <Loading />\r\n            )}\r\n          </Route>\r\n          <Route exact path=\"/customer\">\r\n            <CustomerCenter />\r\n          </Route>\r\n          <Route exact path=\"/itemview/:id\">\r\n            {items ? <ItemView items={items} /> : <Loading />}\r\n          </Route>\r\n          <Route exact path=\"/articleview/:id\">\r\n            {articles ? (\r\n              <ArticleView\r\n                articles={articles}\r\n                userData={userData}\r\n                articleRepository={articleRepository}\r\n              />\r\n            ) : (\r\n              <Loading />\r\n            )}\r\n          </Route>\r\n          <Route exact pat